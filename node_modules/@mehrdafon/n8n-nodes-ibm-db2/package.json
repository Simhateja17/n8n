{
    "name": "@mehrdafon/n8n-nodes-ibm-db2",
    "version": "0.1.0",
    "description": "n8n node for executing queries on IBM DB2",
    "keywords": [
        "n8n-community-node-package"
    ],
    "license": "MIT",
    "homepage": "https://github.com/Simhateja17/n8n",
    "author": {
        "name": "Simhateja",
        "email": "simhateja@example.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Simhateja17/n8n.git"
    },
    "main": "index.js",
    "scripts": {
        "build": "tsc && gulp build:icons",
        "dev": "tsc --watch",
        "format": "prettier --write credentials/**/*.ts nodes/**/*.ts",
        "lint": "eslint credentials/**/*.ts nodes/**/*.ts --fix",
        "prepare": "npm run build"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/IbmDb2Api.credentials.js"
        ],
        "nodes": [
            "dist/nodes/IbmDb2/IbmDb2.node.js"
        ]
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/request-promise-native": "~1.0.21",
        "@typescript-eslint/parser": "~6.21.0",
        "eslint-plugin-n8n-nodes-base": "^1.16.1",
        "gulp": "^4.0.2",
        "n8n-workflow": "^1.49.0",
        "prettier": "^3.2.5",
        "typescript": "~5.4.5"
    },
    "dependencies": {
        "ibm_db": "^3.3.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
